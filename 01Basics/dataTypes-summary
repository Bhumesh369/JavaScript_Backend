// premitive
 // 7 types : String, Number, Boolean, null, undefined, Symbol, BigInt

 /* JS is dynamically typed language */
 const score = 100;
 const scoreValue = 100.4;
 const isLoggedIn = false;
 const outsideTemp = null;
 let userEmail; //undefined, we can assign it undefined as well
 const id = Symbol('123');
 const newId = Symbol('123');
 console.log(id == newId);
 //const bigNumber = 3456328847463628949422334n;
 console.log(typeof bigNumber);
 // Refernce (Non premitive)

 // Array, Objects, Functions

 const heroes = ["Shaktiman", "Nagraj", "Doga"];

 let myObj = {
    name: "Bhumesh",
    age: 30
 }

 const myFunction = function(){
    console.log("Hello Bhumesh")
 }
//+++++++++++++++++++++++++++++++++++++++++++++++++++
 /*
Memory types - 
 Stack memory and Heap Memory

 Stack memory use for premitive data types
 Heap memory use for non-premitive data types

 */

 let myYoutube = "bhumeshparatedotcom";
 let anothername = myYoutube;
 anothername = "Kashuparatedotcom"
 console.log(anothername);
 console.log(myYoutube);
 let user1 = {
    email: "user@gmail.com",
    upi: "user@ybl"
 }
 let user2 = user1;

 user2.email = "kashuparate@google.com";

 console.log(user1.email); // O/P will be 2nd email after change
 console.log(user2.email); // O/P will be 2nd email after change bcs it saves on heap memory.