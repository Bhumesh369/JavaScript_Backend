let name = "Bhumesh"
//number => range 2 to power 53
// bigint => for large number of range
// string => ""
// Boolean => true/false
// null => standalone value (Empty value/null 'Khali')
// undefined => when value in not defined
// symbol => unique

console.log(typeof "Bhumesh");

// object

console.log(typeof undefined); //undefined
console.log(typeof null); //object
